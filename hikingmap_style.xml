<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map[]>
<Map srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over" background-color="#b5d0d0">

<Parameters>
  <Parameter name="name"><![CDATA[Hikingmap]]></Parameter>
  <Parameter name="center">0,0,4</Parameter>
  <Parameter name="format">png</Parameter>
  <Parameter name="metatile">2</Parameter>
  <Parameter name="description"><![CDATA[The hikingmap stylesheet]]></Parameter>
  <Parameter name="scale">1</Parameter>
  <Parameter name="maximum-scale-denominator">22</Parameter>
  <Parameter name="minimum-scale-denominator">0</Parameter>
  <Parameter name="bounds">-180,-85.05112877980659,180,85.05112877980659</Parameter>
</Parameters>

<FontSet name="fontset-hikingmap">
  <Font face-name="DejaVu Sans Book"/>
</FontSet>
<Style name="GPXStyle" filter-mode="first">
  <Rule>
    <LineSymbolizer stroke-width="2" stroke="#ff1493" stroke-opacity="1" />
  </Rule>
</Style>
<Style name="WaypointStyle" filter-mode="first">
  <Rule>
    <TextSymbolizer size="16" fill="#000000" fontset-name="fontset-hikingmap" halo-fill="#ffffff" halo-radius="1" ><![CDATA[[name]]]></TextSymbolizer>
  </Rule>
</Style>

</Map>

